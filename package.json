{
  "name": "eslint-plugin-verb-nouns",
  "version": "1.1.0",
  "description": "ESLint plugin that catches incorrect use of noun forms like 'setup', 'login', 'signup' when verb forms are needed",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btraut/eslint-plugin-verb-nouns.git"
  },
  "bugs": {
    "url": "https://github.com/btraut/eslint-plugin-verb-nouns/issues"
  },
  "homepage": "https://github.com/btraut/eslint-plugin-verb-nouns#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "verb-nouns",
    "copy",
    "ui",
    "setup",
    "login",
    "signup"
  ],
  "author": "Brent Traut",
  "license": "MIT",
  "engines": {
    "node": ">=14.17"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0 || ^10.0.0-alpha"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint src test --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "dev": "vitest watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.47.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.8.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@typescript-eslint/rule-tester": "^8.12.2",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.6"
  },
  "packageManager": "npm@10"
}
